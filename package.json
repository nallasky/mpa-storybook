  "scripts": {
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test:output": "jest --json --outputFile=D:/storybook/src/.jest-test-results.json || true --detectOpenHandles",
    "test:output:watch": "jest --json --outputFile=D:/storybook/src/.jest-test-results.json || true --detectOpenHandles --watch"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ],
    "modulePathIgnorePatterns": [
      "/node_modules/",
      "/src/Design",
      ".jest-test-results.json"
    ],
    "testEnvironment": "jest-environment-jsdom"
  }
